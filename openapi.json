{
  "openapi": "3.0.3",
  "info": {
    "title": "Shopping List API",
    "version": "1.0.0",
    "description": "API for managing shopping lists."
  },
  "servers": [
    {
      "url": "https://api.example.com"
    }
  ],
  "paths": {
    "/lists": {
      "get": {
        "summary": "Get all shopping lists",
        "responses": {
          "200": {
            "description": "A list of shopping lists",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/List"
                  }
                },
                "example": [
                  {
                    "id": 1,
                    "name": "Weekly Groceries",
                    "owner": "john.doe@example.com"
                  },
                  {
                    "id": 2,
                    "name": "Monthly Supplies",
                    "owner": "jane.doe@example.com"
                  }
                ]
              }
            }
          }
        }
      },
      "post": {
        "summary": "Create a new shopping list",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ListInput"
              },
              "example": {
                "name": "Weekly Groceries",
                "owner": "john.doe@example.com"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Shopping list created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/List"
                },
                "example": {
                  "id": 1,
                  "name": "Weekly Groceries",
                  "owner": "john.doe@example.com"
                }
              }
            }
          }
        }
      }
    },
    "/lists/{listId}": {
      "get": {
        "summary": "Get a shopping list by ID",
        "parameters": [
          {
            "name": "listId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "A shopping list",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/List"
                },
                "example": {
                  "id": 1,
                  "name": "Weekly Groceries",
                  "owner": "john.doe@example.com"
                }
              }
            }
          },
          "404": {
            "description": "List not found"
          }
        }
      },
      "put": {
        "summary": "Update a shopping list",
        "parameters": [
          {
            "name": "listId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ListInput"
              },
              "example": {
                "name": "Updated Groceries",
                "owner": "john.doe@example.com"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Shopping list updated",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/List"
                },
                "example": {
                  "id": 1,
                  "name": "Updated Groceries",
                  "owner": "john.doe@example.com"
                }
              }
            }
          },
          "404": {
            "description": "List not found"
          }
        }
      },
      "delete": {
        "summary": "Delete a shopping list",
        "parameters": [
          {
            "name": "listId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Shopping list deleted"
          },
          "404": {
            "description": "List not found"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "List": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "owner": {
            "type": "string"
          }
        }
      },
      "ListInput": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "owner": {
            "type": "string"
          }
        },
        "required": ["name", "owner"]
      }
    }
  }
}
